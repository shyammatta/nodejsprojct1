{
    "name": "employee-management",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "concurrently \"npm run server\" \"npm run client\"",
        "server": "cd backend && npm run dev",
        "client": "cd client && npm start",
        "install-all": "cd backend && npm install && cd ../client && npm install"
    },
    "devDependencies": {
        "concurrently": "^8.2.2",
        "nodemon": "^3.1.7"
    },
    "dependencies": {
        "cross-env": "^7.0.3",
        "multer": "^1.4.5-lts.1",
        "react": "^18.3.1",
        "react-scripts": "^5.0.1"
    }
}
